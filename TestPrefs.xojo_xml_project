   <SourceLine>Sub Resized()</SourceLine>
   <SourceLine>//</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var w As Integer = Self.Width</SourceLine>
   <SourceLine>Var h As Integer = Self.Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>App.Prefs.setIntegerValue("WIDTH", w)</SourceLine>
   <SourceLine>App.Prefs.setIntegerValue("HEIGHT", h)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Window1.GroupBox1.Caption = "Windowsize = " + w.ToString + ", " + h.ToString</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
 </HookInstance>
