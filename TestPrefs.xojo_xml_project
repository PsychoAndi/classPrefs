   <SourceLine>Sub Opening()</SourceLine>
   <SourceLine>// get the window size, if none is there yet, take the default of 400x400</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var w As Integer = App.Prefs.getIntegerValue("WIDTH", 400)</SourceLine>
   <SourceLine>Var h As Integer = App.Prefs.getIntegerValue("HEIGHT", 400)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Window1.Width = w</SourceLine>
