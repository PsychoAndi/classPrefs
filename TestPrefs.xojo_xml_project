   <SourceLine>// key dosn't exist</SourceLine>
   <SourceLine>If rs.RowCount = 0 Then</SourceLine>
   <SourceLine>prefDB.ExecuteSQL("INSERT INTO tblPrefs (key, value) VALUES (?,?);", key.Uppercase, value.StringValue)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Else</SourceLine>
   <SourceLine>// key exists, therefore just update</SourceLine>
   <SourceLine>prefDB.ExecuteSQL("UPDATE tblPrefs SET value=? WHERE key=?;", value.StringValue, key.Uppercase)</SourceLine>
